{
    "version": 2,
    "builds": [
      {
        "src": "api.py",
        "use": "@vercel/python"
      },
      {
        "src": "sticker-generator-frontend",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/generate-sticker",
        "dest": "/api.py"
      },
      {
        "src": "/(.*)",
        "dest": "/sticker-generator-frontend/$1"
      }
    ]
  }
  